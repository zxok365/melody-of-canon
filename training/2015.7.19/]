#include <cstdio>
#include <cstdlib>
#include <algorithm>

using namespace std;

const int N = 205;

int n;

struct Point {
	int x, y, z;

	Point (int x, int y, int z) :
		x(x), y(y), z(z) {}

	void read() {
		scanf("%d %d %d", &x, &y, &z);
	}
} p[N];

int eq(const Point &a, const Point &b) {
	return a.x == b.x && a.y == b.y && a.z == b.z;
}

int dot(const Point &a, const Point &b) {
	return a.x * b.x + a.y * b.y + a.z * b.z;
}

Point det(const Point &a, const Point &b) {
	return Point(a.y * b.z - a.z * b.y, 
				a.z * b.x - a.x * b.z,
				a.x * b.y - a.y * b.x);
}

int main() {
	scanf("%d", &n);
	for (int i = 1; i <= n; i++) {
		p[i].read();
	}

	int ans = 0;
	for (int i = 1; i <= n; i++) {
		for (int j = 1; j <= n; j++) {
			if (i == j) {
				continue;
			}

			Point now = det(p[i], p[j]);
			int s1 = 0, s2 = 0, s3 = 0, s4 = 0, s = 0;
			for (int k = 1; k <= n; k++) {
				if (eq(p[i], p[k]) || eq(p[j], p[k])) {
					s++;
					continue;
				}

				int x = dot(now, p[k]);
				if (x == 0) {
					Point now2 = det(p[i], p[k]);
					int z = dot(now, now2);
					if (z > 0) {
						s3++;
					} else {
						s4++;
					}
				} else if (x > 0) {
					s1++;
				} else {
					s2++;
				}
			}

			ans = max(ans, s + max(s1, s2) + max(s3, s4));
		}
	}

	printf("%d\n", ans);
	return 0;
}

